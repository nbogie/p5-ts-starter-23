{
    "name": "p5-ts-parcel",
    "version": "0.2.0",
    "author": "nbogie",
    "license": "MIT",
    "scripts": {
        "start": "parcel src/index.html",
        "build": "yarn run clean && parcel build src/index.html",
        "clean": "rimraf ./dist ./.parcel-cache",
        "test": "jest --verbose",
        "format": "prettier --write src",
        "type-check": "tsc --noEmit",
        "type-check:watch": "tsc --noEmit --watch"
    },
    "dependencies": {
        "nice-color-palettes": "^3.0.0",
        "p5": "^1.6.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.2",
        "@types/p5": "^1.6.2",
        "jest": "^29.5.0",
        "parcel": "^2.9.2",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.1",
        "ts-jest": "^29.1.0",
        "typescript": "^5.0.4"
    }
}
